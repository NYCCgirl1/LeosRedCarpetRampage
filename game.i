 if (++fanTime % timeForFan == 0 && fanActive < 1) {
  for (int i = 0; i < 10; ++i) {
   if (!fans[i].active) {

    fans[i].col = 240 + fans[i].width*2*i;
    fans[i].minCol = leo.col - leo.size*3;
       fans[i].cdel = -2;
       fans[i].eager = 0;
       fans[i].goingLeft = 1;

    fans[i].active = 1;
    fanActive++;
    fanTime = 0;
    timeForFan = rand()%30 + 50;
    break;
   }
  }
 }


 for (int i = 0; i < 10; i++) {
  if(fans[i].active) {
   fans[i].aniCounter++;


   if(fans[i].col >= fans[i].minCol && fans[i].goingLeft == 1) {
    fans[i].col += fans[i].cdel;
    if(fans[i].aniCounter % 7 == 0) {
     if(fans[i].sprite == 2) {
      fans[i].sprite = 3;
     } else {
      fans[i].sprite = 2;
     }
    }
   } else {
    fans[i].cdel = -1;
    fans[i].col -= fans[i].cdel;
    fans[i].goingLeft = 0;
    fans[i].minCol -= 1;

    if(fans[i].aniCounter % 7 == 0) {
     fans[i].eager++;
     if(fans[i].sprite <= 0) {
      fans[i].sprite = 1;
     } else {
      fans[i].sprite = 0;
     }
    }

    if(fans[i].eager % 20 == 0) {
     fans[i].goingLeft = 1;
    }
   }

   if(fans[i].col < -32) {
    fans[i].active = 0;
    fanActive = 0;
   }
  }
 }
}

void updatePaps1() {
 if (++timer % timeForSnap == 0 && papActive <= 1) {
  for (int i = 0; i < 10; ++i) {
   if(!bendPaps[i].active) {
    bendPaps[i].col = 240 + 32*i;
    bendPaps[i].active = 1;
    papActive++;
    timeForSnap = rand()%37 + 50;
    break;
   }
  }
 }

 for (int i = 0; i < 10; i++) {
  if(bendPaps[i].active) {
   bendPaps[i].col += bendPaps[i].cdel;
   if(leo.aniCounter % 7 == 0) {
    if(bendPaps[i].sprite <= 0) {
     bendPaps[i].sprite = 1;
    } else {
     bendPaps[i].sprite = 0;
    }
   }
  }

  if(bendPaps[i].col < -32) {
   papActive = 0;
   bendPaps[i].active = 0;
  }
 }
}

void updateAwards() {
 for(int i = 0; i < 35; i++) {
  if(++timer % 213 && awardActive <= 3) {
   if(!awards[i].active) {
    awards[i].col = 240 + 16*i;
    awards[i].active = 1;
    awardActive++;
    break;
   }
  }

  if(awards[i].active) {
   if(leo.aniCounter % 7 == 0) {
    if(awards[i].anistate%2 == 0) {
     awards[i].anistate++;
    } else {
     awards[i].anistate--;
    }
   }
   awards[i].col += awards[i].cdel;

   if (collision(((leo.row) >> 8), leo.col, leo.size, leo.size,
   awards[i].row, awards[i].col, awards[i].height, awards[i].width)) {
    awards[i].active = 0;
    awardsCollected+=1;
    if(awardsCollected >= 10) {
      drawScore(1, (awardsCollected/10));
     }
     drawScore(0, (awardsCollected%10));
    awardActive--;
   }
   if(awards[i].col < -32) {
    awards[i].active = 0;
    awardActive--;
   }
  }
 }
}


void drawGame() {

 if(level == 1) {
  drawPaps();
 }
 if(level == 2) {
  drawFans();
  drawPaps();
 }
 if(level == 3) {
  drawFans();
  drawPaps();
  drawAwards();

  for(int i = 0; i < 3; i++) {
   int col = 180 - (3-i)*16;
   int ind = 50 + i;
   shadowOAM[ind].attr0 = (145 & 0xFF) | (0<<13) | (0<<14);
   shadowOAM[ind].attr1 = (col & 0x1FF) | (1<<14);
   shadowOAM[ind].attr2 = ((20)*32+((i*2) + 26));
  }

  for(int i = 0; i < 2; i++) {
   int ind = 56+i;
   int col = 200 + i*16;
   shadowOAM[ind].attr0 = (145 & 0xFF) | (0<<13) | (0<<14);
   shadowOAM[ind].attr1 = (col & 0x1FF) | (1<<14);
   shadowOAM[ind].attr2 = ((22)*32+((i*2) + 26));
  }

 }
 for(int i = 0; i < (lifeCount + 1); i++) {
     drawLives(&lives[i]);
 }
 drawPlayer();
 DMANow(3, shadowOAM, ((OBJ_ATTR*)(0x7000000)), 128*4);
}



