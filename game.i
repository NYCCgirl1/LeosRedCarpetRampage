 shadowOAM[0].attr0 = (((leo.row) >> 8) & 0xFF)| (0<<13) | (0<<14);
 shadowOAM[0].attr1 = (leo.col & 0x1FF) | (2<<14);
 shadowOAM[0].attr2 = ((leo.sprite*4)*32+(leo.anistate*4));

}

